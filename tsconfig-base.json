{
	"compilerOptions": {
		"target": "ES2022",
		"module": "ESNext",
		"declaration": true,
		"declarationMap": true,
		"sourceMap": true,
		"composite": true,
		"strict": true,
		"moduleResolution": "bundler",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"verbatimModuleSyntax": true
	}
}
